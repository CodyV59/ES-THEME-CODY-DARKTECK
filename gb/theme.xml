<theme>
	<formatVersion>7</formatVersion>
	<include>./../theme.xml</include>

	<view name="system">
		<image name="logo">
			<path>./system.png</path>
		</image>
		<image name="background_icon" extra="true">
         <origin>0 0</origin>
         <pos>0 0</pos>
         <size>1 1</size>
         <zIndex>3</zIndex>                 
         <path>./background_icon.png</path>
      </image>
	</view>	
	
  	<view name="detailed, video">
                <image name="logo" extra="true">
                   <origin>0 0</origin>
                   <pos>0 0</pos>
                   <maxSize>1 1</maxSize>
                   <path>./logo.png</path>
                   <zIndex>3</zIndex>            
                </image>
                <image name="gbeffect" extra="true">
                   <origin>0 0</origin>
                   <pos>0 0</pos>
                   <maxSize>1 1</maxSize>
                   <path>./gridgb.png</path>
                   <zIndex>2</zIndex>            
                </image>
	</view>	
  	<view name="basic">
                <image name="system" extra="true">
                   <origin>0.5 0</origin>
                   <pos>0.5 0.02</pos>
                   <maxSize>1 .12</maxSize>
                   <path>./logo.png</path>
                   <zIndex>3</zIndex>            
                </image>
	</view>		
</theme>
